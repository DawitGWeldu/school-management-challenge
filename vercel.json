{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { 
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/(css|js)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "School Management",
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://school-management-challenge.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "ep-polished-star-a590y51g-pooler.us-east-2.aws.neon.tech",
        "DB_PORT": "5432",
        "DB_DATABASE": "schoolmanagement",
        "DB_USERNAME": "db_owner"
    }
} 